	};
	JSON = {
	    Key = "JSON";
	    Text = "Can't parse JSON";
	    String = "Issue was fixed rejoin and reload Lunar-X.";
	};
	Par = {
	    Key = "Par";
	    Text = "Lunar-X not being up to par";
	    String = "Executed latest script and set newest loadstring to your clipboard, if your running Lunar-X right after an update please consider waiting some time before running again.";
	};
	Retardation = {
	    Key = "Ret";
	    Text = "Retardation user error";
	    String = "Please screw in your brain.";
	};
	Forbidden = {
